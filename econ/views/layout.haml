%html
	%head
		%title Econ
		%script{ :type => "text/javascript", :src => "http://static.wowhead.com/widgets/power.js"}
		%link{ :href => "/style.css", :rel => "stylesheet", :type => "text/css"}
		%link{ :href => "/extra.css", :rel => "stylesheet", :type => "text/css"}
	%body
		#body
			#wrapper
				%header#header.clearfix
					%h1.left
